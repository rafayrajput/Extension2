{
    "manifest_version": 3,
    "name": "Funny Joke Extension",
    "version": "1.0",
    "description": "Get a random funny joke!",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": ["scripting"]
  }  